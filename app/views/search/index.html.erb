<div class='container'>
  Total stores: <%= @total %>
  <br>
  <% @stores.each do |store| %>
    <p>Long Name: <%= link_to store.long_name, "/search/#{store.id}" %></p>
    <p>City: <%= store.city %></p>
    <p>State: <%= store.state %></p>
    <p>Phone: <%= store.phone %></p>
    <p>Store Type: <%= store.store_type %></p>
    <p>Distance: <%= store.distance %></p>
    <br><br>
  <% end %>
</div>
